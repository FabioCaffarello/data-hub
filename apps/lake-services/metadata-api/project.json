{
  "name": "lake-services-metadata-api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/lake-services/metadata-api/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/lake-services/metadata-api",
        "main": "apps/lake-services/metadata-api/src/main.ts",
        "tsConfig": "apps/lake-services/metadata-api/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/lake-services/metadata-api/src/assets",
            "output": "/"
          }
        ],
        "tsPlugins": [
          {
            "name": "@nestjs/swagger/plugin",
            "options": {
              "dtoFileNameSuffix": [".dto.ts", ".model.ts"]
            }
          }
        ]
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/lake-services/metadata-api/src/environments/environment.ts",
              "with": "apps/lake-services/metadata-api/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "lake-services-metadata-api:build"
      },
      "configurations": {
        "production": {
          "buildTarget": "lake-services-metadata-api:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/lake-services/metadata-api/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/lake-services/metadata-api/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
