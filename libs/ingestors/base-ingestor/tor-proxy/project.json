{
  "name": "ingestors-base-ingestor-tor-proxy",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ingestors/base-ingestor/tor-proxy/torproxy",
  "targets": {
    "docs": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "pydoc-markdown -p torproxy --render-toc > docs/source/api.md",
        "cwd": "libs/ingestors/base-ingestor/tor-proxy"
      }
    },
    "lock": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "poetry lock --no-update",
        "cwd": "libs/ingestors/base-ingestor/tor-proxy"
      }
    },
    "add": {
      "executor": "@nxlv/python:add",
      "options": {}
    },
    "update": {
      "executor": "@nxlv/python:update",
      "options": {}
    },
    "remove": {
      "executor": "@nxlv/python:remove",
      "options": {}
    },
    "build": {
      "executor": "@nxlv/python:build",
      "outputs": ["libs/ingestors/base-ingestor/tor-proxy/dist"],
      "options": {
        "outputPath": "libs/ingestors/base-ingestor/tor-proxy/dist",
        "publish": true
      }
    },
    "install": {
      "executor": "@nxlv/python:install",
      "options": {
        "silent": false,
        "args": "",
        "cacheDir": ".cache/pypoetry",
        "verbose": false,
        "debug": false
      }
    },
    "lint": {
      "executor": "@nxlv/python:flake8",
      "outputs": ["reports/libs/ingestors/base-ingestor/tor-proxy/pylint.txt"],
      "options": {
        "outputFile": "reports/libs/ingestors/base-ingestor/tor-proxy/pylint.txt"
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [
        "reports/libs/ingestors/base-ingestor/tor-proxy/unittests",
        "coverage/libs/ingestors/base-ingestor/tor-proxy"
      ],
      "options": {
        "command": "poetry run pytest tests/",
        "cwd": "libs/ingestors/base-ingestor/tor-proxy"
      }
    },
    "run-tor": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "docker build -t datahub/tor_proxy . && docker run -d --restart always -p 9050:9050 --name torproxy datahub/tor_proxy",
        "cwd": "libs/ingestors/base-ingestor/tor-proxy"
      }
    },
    "run-ip": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "poetry run python3 torproxy/ip.py",
        "cwd": "libs/ingestors/base-ingestor/tor-proxy"
      }
    }
  },
  "tags": []
}
