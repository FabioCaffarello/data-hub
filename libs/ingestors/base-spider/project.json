{
  "name": "ingestors-base-spider",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/ingestors/base-spider/basespider",
  "targets": {
    "lock": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "poetry lock --no-update",
        "cwd": "libs/ingestors/base-spider"
      }
    },
    "add": {
      "executor": "@nxlv/python:add",
      "options": {}
    },
    "update": {
      "executor": "@nxlv/python:update",
      "options": {}
    },
    "remove": {
      "executor": "@nxlv/python:remove",
      "options": {}
    },
    "build": {
      "executor": "@nxlv/python:build",
      "outputs": ["libs/ingestors/base-spider/dist"],
      "options": {
        "outputPath": "libs/ingestors/base-spider/dist",
        "publish": false
      }
    },
    "install": {
      "executor": "@nxlv/python:install",
      "options": {
        "silent": false,
        "args": "",
        "cacheDir": ".cache/pypoetry",
        "verbose": false,
        "debug": false
      }
    }
  },
  "tags": []
}
