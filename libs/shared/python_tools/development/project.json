{
  "name": "shared-python-tools-development",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/shared/python_tools/development/shared_development",
  "targets": {
    "lock": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "poetry lock --no-update",
        "cwd": "libs/shared/python_tools/development"
      }
    },
    "add": {
      "executor": "@nxlv/python:add",
      "options": {}
    },
    "update": {
      "executor": "@nxlv/python:update",
      "options": {}
    },
    "remove": {
      "executor": "@nxlv/python:remove",
      "options": {}
    },
    "install": {
      "executor": "@nxlv/python:install",
      "options": {
        "silent": false,
        "args": "",
        "cacheDir": ".cache/pypoetry",
        "verbose": false,
        "debug": false
      }
    },
    "test": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [
        "{workspaceRoot}/reports/libs/shared/python_tools/development/unittests",
        "{workspaceRoot}/coverage/libs/shared/python_tools/development"
      ],
      "options": {
        "command": "poetry run pytest tests/",
        "cwd": "libs/shared/python_tools/development"
      }
    }
  },
  "tags": []
}
