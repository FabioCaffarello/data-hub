{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "data-hub",
  "affected": {
    "defaultBase": "origin/main"
  },
  "plugins": [
    "@nx-go/nx-go"
  ],
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*",
    "go.mod": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ],
        "useDaemonProcess": false,
        "accessToken": "NWI5MGJiOWUtNTc1OC00YThkLThjN2YtMjQyOTJiOTE3NWIzfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json"
      ]
    },
    "test": {
      "inputs": [
        "default",
        "^default",
        "{workspaceRoot}/jest.preset.js"
      ]
    }
  },
  "namedInputs": {
    "default": [
      "{projectRoot}/**/*",
      "sharedGlobals"
    ],
    "sharedGlobals": [
      "{workspaceRoot}/go.mod"
    ],
    "production": [
      "default"
    ]
  }
}
